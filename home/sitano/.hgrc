[tortoisehg]
ui.language = en
tabwidth = 4
defaultwidget = commit
shell = gnome-terminal
authorcolor = True
editor = gedit $FILE

[ui]
username = John.Koepi <john.koepi@gmail.com>

[extensions]
color =
hgext.convert = 
fetch = 
gpg = 
graphlog = 
interhg = 
keyword = 
mq = 
progress =
purge = 
rebase = 
transplant = 
churn = 
eol = 
hgk = 
highlight = 
record = 
share = 

[web]
cacerts = /etc/ssl/certs/ca-certificates.crt
encoding = UTF-8
push_ssl = False

[diff]
nodates = True
showfunc = True
ignorews = False
ignorewsamount = False
ignoreblanklines = False

[insecurehosts]
libra.neodinamika.com = 1
bitbucket.org = 1
